<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.home_android_interface">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Home_Android_Interface">
        <activity android:name=".MainPage" android:theme="@style/AppTheme.ActionBar.Transparent">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login"></activity>
        <activity android:name=".TabActivity" android:theme="@style/AppTheme.ActionBar.Transparent"></activity>
        <activity android:name=".UserRegistration"></activity>
        <activity android:name=".MainActivity" android:theme="@style/AppTheme.ActionBar.Transparent"></activity>
        <activity android:name=".DirectWebpage" android:theme="@style/AppTheme.ActionBar.Transparent"></activity>
        <activity android:name=".NotificationView"
            android:label="@string/notification_activity"> </activity>
        <activity android:name=".Notification">
        </activity>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>